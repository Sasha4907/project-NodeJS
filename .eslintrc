{
  "extends": [
    "react-app", 
    "react-app/jest",
    "airbnb/base",
    "plugin:node/recommended"
    ],
    "rules": {
        "semi": "error",
        "no-console": "off",
        "no-unused-vars": "off",
        "consistent-return":"off",
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "max-len": ["error", 200, {"ignoreUrls": true}]
    }
}